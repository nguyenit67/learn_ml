   "source": [
    "def sum_(a, begin, end):\n",
    "    if begin > end:\n",
    "        return 0\n",
    "\n",
    "    if begin == end:\n",
    "        return a[begin]\n",
    "\n",
    "    t = (begin + end) // 2\n",
    "    return sum_(a, begin, t) + sum_(a, t + 1, end)\n",
    "\n",
    "\n",
    "arr = [5, 43.2, 2, 3.4]\n",
    "sum_(arr, 0, len(arr) - 1)"
   ]
  }
